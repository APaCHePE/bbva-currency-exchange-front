{
  "root": true,
  "env": {
    "node": true,
    "browser": true,
    "es2021": true,
    "vue/setup-compiler-macros": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "parser": "@babel/eslint-parser",
    "requireConfigFile": false
  },
  "rules": {
    "vue/multi-word-component-names": "off",
    "no-unused-vars": "warn",
    "vue/no-unused-components": "warn",
    "prettier/prettier": [
      "warn",
      {
        "endOfLine": "auto"
      }
    ]
  }
}
